{
  "name": "passplum",
  "version": "1.0.0",
  "description": "Easier, Better Passwords",
  "scripts": {
    "deploy": "now --public -e NODE_ENV=production -e ROLLBAR_ACCESS_TOKEN=@pp-rollbar-access-token -e DATABASE_URL=@pp-db-url -e REDIS_URL=@pp-redis-url -e CRYPTO_SALT=@pp-crypto-salt",
    "start": "node lib/",
    "test": "lab -c -L -t 77 -r console -o stdout -r lcov -o lcov.info",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maxbeatty/passplum.git"
  },
  "author": "Max Beatty",
  "license": "MIT",
  "dependencies": {
    "boom": "2.9.0",
    "handlebars": "4.0.5",
    "hapi": "11.1.4",
    "inert": "3.0.1",
    "joi": "6.7.1",
    "pg": "6.1.0",
    "pg-hstore": "2.3.2",
    "ratelimiter": "2.0.1",
    "redis": "2.0.1",
    "rollbar": "0.6.2",
    "sequelize": "3.24.1",
    "vision": "3.0.0",
    "zxcvbn": "3.2.2",
    "snyk": "^1.30.1"
  },
  "devDependencies": {
    "code": "3.0.1",
    "lab": "10.8.2",
    "proxyquire": "1.7.1",
    "sinon": "1.16.1"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "private": true,
  "snyk": true
}
